[*]
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = space
insert_final_newline = true
max_line_length = 150
tab_width = 2
ij_continuation_indent_size = 2

[{*.ats, *.ts, *.tsx}]
ij_typescript_import_prefer_absolute_path = true

[{*.cjs, *.js, *.jsx}]
ij_javascript_import_prefer_absolute_path = true
